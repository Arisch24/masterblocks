{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "masterblocks/copy-block",
	"version": "0.1.0",
	"title": "Copy Block",
	"category": "masterblocks",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"color": {
			"background": true,
			"text": true
		},
		"anchor": true
	},
	"attributes": {
		"buttonName": {
			"type": "string",
			"default": "Copy JSON"
		},
		"jsonData": {
			"type": "string"
		},
		"btnTextColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"btnBgColor": {
			"type": "string",
			"default": "#000000"
		}
	},
	"textdomain": "copy-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
